    "version": "0.2.0",
    "configurations": [
        {
            "name": "widgetbook",
            "cwd": "example",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define-from-file",
                ".env"
            ]
        },
        {
            "name": "test",
            "cwd": "test",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define-from-file",
                ".env"
            ]
        },
        {
            "name": "Test",
            "type": "dart",
            "request": "launch",
            "codeLens": {
              "for": [
                "run-test",
                "run-test-file",
                "debug-test",
                "debug-test-file"
              ],
              "path": "test",
              "title": "${debugType}"
            },
            "args": [
                "--dart-define-from-file",
                "example/.env"
            ]
        },
       
    ]
}